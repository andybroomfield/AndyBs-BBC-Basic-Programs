   10 MODE 1
   20 VDU 23,255,255,129,255,255,126,255,255,126
   30 DIM HS(6):DIM HSN$(6) 
   40 DIM BBB(60)
   50 DIM L(650)
   60 PP=1
   70 FOR K=1 TO 650
   80 N=RND(3)
   90 IF PP=1 THEN N=2
  100 IF PP>25 THEN N=1
  110 IF N=1 THEN L(K)=PP-1
  120 IF N=2 THEN L(K)=PP+1
  130 IF N=3 THEN L(K)=PP
  140 PP=L(K)
  150 PRINT TAB(1,2);650-K;" "
  160 NEXT
  170 FOR K=1 TO 6
  180 PPP=PPP+1
  190 HS(K)=0:HSN$(K)="S[()RPI()N"
  200 NEXT
  210 GOTO 520
  220 INPUT "DRIVER";DRIVER$
  230 FOR K=1 TO 23
  240 PRINT TAB(L(K))"!";TAB(L(K)+11)"!" 
  250 NEXT 
  260 S=1:C=5:BOUNUS=0:CB=30
  270 IF S>650-23 THEN GOTO 300
  280 BBB(CB)=RND(9)+L(S+23)
  290 BBB(CB-23)=BBB(CB)
  300 A$=INKEY$(0)
  310 IF A$="Z" THEN C=C-1
  320 IF A$="X" THEN C=C+1
  330 IF C<=L(S) OR C>=L(S)+11 THEN 440 
  340 IF C=BBB(CB-22) THEN BOUNUS=BOUNUS+S:SOUND 1,-15,100,1 
  350 PRINT TAB(0,23)""
  360 IF S>650-23 THEN PRINT:GOTO 390   
  370 PRINT TAB(L(S+23))"!";TAB(BBB(CB))".";TAB(L(S+23)+11)"!" 
  380 CB=CB+1:IF CB>60 THEN CB=30
  390 PRINT TAB(C,0)CHR$(255)  
  400 FOR TD=1 TO 10000:NEXT
  410 S=S+1
  420 IF S=649 THEN 440 
  430 GOTO 270
  440 IF S>648 THEN PRINT "FINISH" ELSE PRINT "CRASHED!!!" 
  450 S=S+BOUNUS:W=6:PRINT "SCORE=";S:PRINT "ASSESING SCORE"
  460 FOR K=5 TO 1 STEP-1  
  470 IF S>HS(K) THEN HSN$(K)=HSN$(K-1):W=K
  480 IF S>HS(K) THEN HS(K)=HS(K-1)
  490 NEXT K
  500 IF W=0 THEN 520
  510 HS(W)=S:HSN$(W)=DRIVER$
  520 FOR K=1 TO 1000000:NEXT:PRINT "PRESS A KEY":A$=GET$ 
  530 CLS:PRINT "CAR VERSION TWO - THE NEXT INCARNATION"
  540 PRINT:PRINT "BY ANDREW(SCORPION)BROOMFIELD"
  550 PRINT :PRINT "Z=LEFT X=RIGHT":PRINT:PRINT "HIGH SCORES"  
  560 FOR K=1 TO 5
  570 PRINT K;"=";HSN$(K);"=";HS(K)
  580 NEXT
  590 PRINT "PLAY (Y/N)"
  600 A$=GET$
  610 IF A$="Y" THEN 220
  620 IF A$<>"N" THEN 600
  630 MODE7
  660 END
